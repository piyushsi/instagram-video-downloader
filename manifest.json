{
  "icons": {
    "16": "src/icon16.png",
    "48": "src/icon48.png",
    "128": "src/icon128.png"
  },
  "manifest_version": 2,
  "name": "Instagram Video Downloader",
  "description": "Download any Instagram Video from official Site",
  "version": "2.1.0",
  "author": "Piyush Sinha",
  "permissions": [
    "downloads"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.instagram.com/*"],
      "js": ["src/loader.js"]
    }
  ]
}
